{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "typeOrm": {
    "host": "DB_HOST",
    "port": "DB_PORT",
    "username": "DB_USER",
    "password": "DB_PASSWORD",
    "database": "DB_NAME",
    "logging": "DB_LOGGING",
    "enableSslAuth": "DB_SSL_ENABLE",
    "sslPaths": {
      "ca": "DB_SSL_CA",
      "key": "DB_SSL_KEY",
      "cert": "DB_SSL_CERT"
    }
  }
}
